{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Server",
      "program": "${workspaceFolder}/index.js",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    },
    {
      "type": "pwa-node",
      "request": "attach",
      "name": "Attach to Server",
      "port": 9229,
      "restart": true,
      "protocol": "inspector"
    }
  ]
}